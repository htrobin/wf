function InStreamParam(msg){
	//msg = "项目: 10171 的请购单被拒绝，请重新提交！"
	//获取页面所有数据
	msg = encodeURI(encodeURI(msg));
	//获取路径信息
	var path = "http://115.28.236.123/wechat/api.jsp";
	$.ajax({
			url : path,
			type : "get",
			dataType : "text",
			data: {
				openid:"oj3SBjpeACUt1oRBRSZRxdXQCtWo",
				account:"gh_f1f65fcea9cc",
				msg:msg,
				accesstoken:"bFNstWJYBhWpwpVVgiI2FM-h9LbV4gtVQnbq8iuUnvNXjsL8biK1afzo7JgFIdVc-p5NMD8hjrc4oYKqnaHbab9BPd-Anj1eR12OX6_b184"
			},
			success : function (data){
				
		 	},
			error: function (e,m){
		 		alert(e);
		 		alert(m);
		 	}
	});
}
function wfSubmitDoc(){
	if(arguments.length==1){gWQSagent=arguments[0]}
	wfSubDocStart();
}
function wfSaveDoc() {
	/*不做任何操作保存文档*/
	if(confirm("确定保存文档吗？")){
		try{gForm.WFFlowLogXML.value=dojox.xml.parser.innerXML(gWFLogXML)}catch(e){};
		fnResumeDisabled(); //恢复部分域的失效状态，以保证保存时值不会变为空。
		gForm["$$QuerySaveAgent"].value="(wqsWFSave)";
		gForm.submit();
		//wfSubDocEndSave();
	}
}
function wfAgreeDoc(){
	//提交前处理数据
	if(gForm.WFStatus.value==0){SubmitBefore()}
	if(confirm("您确定同意吗？")){
		if(arguments.length==1){gWQSagent=arguments[0]}
		gForm.WFTacheName.value="同意";
		wfSubDocStart();
	}
}
function wfConfirmDoc(){
	//提交前处理数据
	if(gForm.WFStatus.value==0){SubmitBefore()}
	if(confirm("您确定确认吗？")){
		if(arguments.length==1){gWQSagent=arguments[0]}
		gForm.WFTacheName.value="确认";
		wfSubDocStart();
	}
}
function wfCompleatDoc(){
	//提交前处理数据
	if(gForm.WFStatus.value==0){SubmitBefore()}
	if(confirm("您确定完成吗？")){
		if(arguments.length==1){gWQSagent=arguments[0]}
		gForm.WFTacheName.value="完成";
		wfSubDocStart();
	}
}
function wfRejectDoc(){
	if(confirm("您确定拒绝吗？")){
		if(arguments.length==1){gWQSagent=arguments[0]}
		gForm.WFTacheName.value="拒绝";
		wfSubDocStart();
	}
}
function goBak(){
	if(dojo.byId("WFIdea")){
		if(dojo.trim(dojo.byId("WFIdea").value)==""){
			dojo.byId("WFIdea").style.borderColor="red";
			dijit.byId("tabAttachContent").selectChild("tabWFIdea");
			if(!dijit.byId("expp")._showing){
				dijit.byId("expp").toggle();
			}
			alert("请您填写拒绝意见！");
			return;
		}
	}
	var idea=dojo.trim(dojo.byId("WFIdea").value);
	var link="/"+gCommonDB+"/fmWFRandomBack?OpenForm&DocID="+gCurDocID+"&TDB="+gCurDBName+"&idea="+encodeURI(idea)+"&idearid="+gIdeaID[0];
	dg.set("content","<iframe style='width:580px;height:400px;border:0px;' src='"+link+"' frameborder='0'></iframe>");
	dg.show();
}
/*
描述：
	选择部门后给页面的设置域的实际值和显示值
参数：
	e：部门树节点对象
*/
function SelectOrgDept(e){
	//fullname:部门层次名,name:部门名称
	e.sender.setValue(e.node.fullname);
	e.sender.setText(e.node.name);
}
/*
描述：
	选择部门中加入清除图标，点击清除值
参数：
	e：部门树节点对象
*/
function onCloseClick(e) {
	e.sender.setValue("");
	e.sender.setText("");
}
