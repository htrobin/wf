	with(document.forms[0]){
		TANGER_OCX_SaveDoc(TANGER_OCX_filename.value,false);
	}
}
function goSubmit(){
	with(document.forms[0]){
		TANGER_OCX_SaveDoc(TANGER_OCX_filename.value,true);
	}
}
